{
  "name": "lh-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/lh-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/lh-app",
        "index": "apps/lh-app/src/index.html",
        "browser": "apps/lh-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/lh-app/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/lh-app/src/favicon.ico",
          "apps/lh-app/src/assets",
          {
            "input": "libs/web/components/src/assets",
            "glob": "**/*",
            "output": "assets/components"
          }

        ],
        "styles": [
          "apps/lh-app/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": [ "libs/web/components/src/assets/styles" ]
      },
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "lh-app:build:production"
        },
        "development": {
          "buildTarget": "lh-app:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/lh-app/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "lh-app:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/lh-app/jest.config.ts"
      }
    }
  }
}
